aside.main-sidebar
    // sidebar: style can be found in sidebar.less
    section.sidebar
        // Sidebar user panel
        .user-panel
            .pull-left.image
                img.img-circle(src='/adminlte/dist/img/user2-160x160.jpg' alt='User Image')
            .pull-left.info
                p #{session_store.username}
                a(href='#')
                    i.fa.fa-circle.text-success
                    |  Online
        ul.sidebar-menu(data-widget='tree')
            li.header MAIN NAVIGATION
                li.treeview
                    a(href='/')
                        i.fa.fa-dashboard
                        span Dashboard
                if(session_store.admin == true)
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Nilai Mahasiswa
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputbuku')
                                    i.fa.fa-circle-o
                                    |  Input Nilai Mahasiswa

                            li
                                a(href='/databuku')
                                    i.fa.fa-circle-o
                                    |  Lihat Nilai Mahasiswa
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data DOSEN
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputdosen')
                                    i.fa.fa-circle-o
                                    |  Input Data Dosen

                            li
                                a(href='/datadosen')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Dosen
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Mahasiswa
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputmahasiswa')
                                    i.fa.fa-circle-o
                                    |  Input Data Mahasiswa

                            li
                                a(href='/datamahasiswa')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Mahasiswa
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Kalender Akademik
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputtgl')
                                    i.fa.fa-circle-o
                                    |  Input Kalender Akademik

                            li
                                a(href='/datatgl')
                                    i.fa.fa-circle-o
                                    |  Lihat Kalender Akademik

                else
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Nilai Mahasiswa
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/databukumember')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Nilai
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Dosen
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/datadosenmember')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Dosen
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Mahasiwa
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/datamahasiswamember')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Mahasiswa

    // /.sidebar